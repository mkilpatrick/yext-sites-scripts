{
  "name": "@yext/yext-sites-scripts",
  "version": "0.0.1",
  "description": "The default React development toolchain provided by Yext",
  "keywords": [
    "Yext",
    "React"
  ],
  "author": "Yext",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "dev": "node --loader=ts-node/esm --experimental-specifier-resolution=node entry-server.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "release": "npm-run-all release:* --parallel release:compile:*",
    "release:clean": "rm -rf dist && mkdir dist",
    "release:copy": "cp vite.config.ts dist/ && sed -e '/\"private\"/d' package.json > dist/package.json",
    "release:compile:esm": "tsc",
    "prepare": "npm run release",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A",
    "postversion": "git push && git push --tags",
    "format": "prettier --write \"yext-sites-scripts/**/*.ts\" \"yext-sites-scripts/**/*.tsx\" \"server.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "bin": {
    "yext-sites-scripts": "dist/bin/yext-sites-scripts.js"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "esm-module-paths": "^1.1.1",
    "express": "^4.17.3",
    "shelljs": "^0.8.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.18",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.10",
    "@types/shelljs": "^0.8.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  },
  "peerDependencies": {
    "@vitejs/plugin-react": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "vite": "^2.8.6"
  }
}