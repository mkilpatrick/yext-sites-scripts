{
  "name": "yext-sites-scripts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/yext-sites-scripts/context.js"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "dev": "node --loader=ts-node/esm --experimental-specifier-resolution=node entry-server.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "release": "npm-run-all release:* --parallel release:compile:*",
    "release:clean": "rm -rf dist && mkdir dist",
    "release:copy": "cp index.d.ts vite.config.ts dist/ && sed -e '/\"private\"/d' package.json > dist/package.json",
    "release:compile:esm": "tsc",
    "prepare" : "npm run release"
  },
  "bin": {
    "yext-sites-scripts": "dist/bin/yext-sites-scripts.js"
  },
  "files": ["dist/**/*"],
  "dependencies": {
    "express": "^4.17.3",
    "shelljs": "^0.8.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "esm-module-paths": "^1.1.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.18",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.10",
    "@types/express": "^4.17.13",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "vite": "^2.8.6",
    "@vitejs/plugin-react": "^1.2.0"
  }
}
